                                    </li>

                                    <!--Universidades Section-->
                                    <li>
                                        <a href="@Url.Action("index","universidades")">Universidades</a>
                                    </li>

                                   
                                    <!-- Log Off section Link-->
                                    <li>
                                        <a href="#">
                                        <span class="glyphicon glyphicon-log-out"></span>
                                        Cerrar Sesión
                                        </a>
                                    </li>

                                </ul>
                            </div>
                        </div>
                        <div class="rd-navbar-aside">
                           <!--We removed the social network links-->
                        </div>
                    </div>
                </div>
